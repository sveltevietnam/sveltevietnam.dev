{
	"name": "@sveltevietnam/kit",
	"version": "1.0.0-next",
	"description": "reusable bits for Svelte Vietnam projects",
	"private": true,
	"main": "src/index.js",
	"module": "src/index.js",
	"types": "types/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		},
		"./components": {
			"types": "./types/index.d.ts",
			"import": "./src/components/index.js"
		},
		"./constants": {
			"types": "./types/index.d.ts",
			"import": "./src/constants.js"
		},
		"./contexts": {
			"types": "./types/index.d.ts",
			"import": "./src/contexts/index.js"
		},
		"./dialogs": {
			"types": "./types/index.d.ts",
			"import": "./src/dialogs/index.js"
		},
		"./hooks": {
			"types": "./types/index.d.ts",
			"import": "./src/hooks.js"
		},
		"./notifications": {
			"types": "./types/index.d.ts",
			"import": "./src/notifications/index.js"
		},
		"./utilities": {
			"types": "./types/index.d.ts",
			"import": "./src/utilities/index.js"
		},
		"./utilities/structured-data": {
			"types": "./types/index.d.ts",
			"import": "./src/utilities/structured-data/index.js"
		},
		"./utilities/datetime": {
			"types": "./types/index.d.ts",
			"import": "./src/utilities/datetime.js"
		},
		"./assets/images/logos/*.base64": "./src/assets/images/logos/*.base64",
		"./assets/images/fallbacks/*.jpg": "./src/assets/images/fallbacks/*.jpg"
	},
	"publishconfig": {
		"access": "public",
		"provenance": true
	},
	"files": [
		"src",
		"types"
	],
	"license": "MIT",
	"author": {
		"email": "developers@sveltevietnam.dev",
		"name": "Svelte Vietnam",
		"url": "https://github.com/sveltevietnam"
	},
	"homepage": "https://www.sveltevietnam.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sveltevietnam/sveltevietnam.dev.git",
		"directory": "packages/kit"
	},
	"bugs": {
		"url": "https://github.com/sveltevietnam/sveltevietnam.dev/issues"
	},
	"keywords": [
		"svelte",
		"design system",
		"resuable",
		"utilities",
		"components",
		"constants"
	],
	"scripts": {
		"prepublishOnly": "dts-buddy && publint",
		"dts": "dts-buddy && publint",
		"lint": "run-p lint:*",
		"lint:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"lint:es": "eslint .",
		"lint:style": "stylelint '**/*.{svelte,css}'",
		"format": "prettier --write --ignore-path=../../.prettierignore --config=../../prettier.config.js ."
	},
	"peerDependencies": {
		"svelte": "^5.16.6"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250807.0",
		"@svelte-put/lockscroll": "^2.0.1",
		"@sveltejs/kit": "^2.27.3",
		"@sveltevietnam/css": "workspace:*",
		"@tailwindcss/vite": "4.1.11",
		"schema-dts": "^1.1.5",
		"svelte": "^5.38.0",
		"tailwindcss": "4.1.11",
		"vite": "^7.1.1",
		"vite-imagetools": "^7.1.0"
	},
	"dependencies": {
		"@svelte-put/async-stack": "^1.5.1",
		"@svelte-put/clickoutside": "^4.0.0",
		"@svelte-put/copy": "^4.0.0",
		"@svelte-put/qr": "^2.1.0",
		"@sveltevietnam/i18n": "workspace:*",
		"defu": "^6.1.4"
	}
}
